<Window x:Class="Warehouse_b.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Warehouse_b"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid Background="YellowGreen" Margin="2" >
        <StackPanel >
            <Label Content="LOGIN" Foreground="White" FontSize="25" HorizontalAlignment="Center"/>
            <Separator></Separator>
            <Label Content="Username" Foreground="White" FontSize="30"/>
            <TextBox x:Name="txtUsername" Margin="20,20,283.6,20" Height="48" RenderTransformOrigin="0.578,1.685"/>
            <Label Content="Paswoord" Foreground="White" FontSize="30" />
            <TextBox x:Name="txtPaswoord" Margin="20,30,283.6,50" Height="48"/>
            <Button x:Name="btnSubmit" Content="Submit" Margin="594,0,48.6,0" Click="btnSubmit_Click" Height="37"/>

        </StackPanel>
    </Grid>
</Window>
